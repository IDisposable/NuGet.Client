{
  "version": "3.5.0-*",
  "description": "NuGet 3 restore for dotnet CLI, DNX, and UWP",
  "compilationOptions": {
    "emitEntryPoint": true,
    "warningsAsErrors": true
  },
  "compile": [
    "../NuGet.Shared/*.cs"
  ],
  "dependencies": {
    "Microsoft.Extensions.PlatformAbstractions": "1.0.0-rc2-20100",
    "Microsoft.Extensions.CommandLineUtils.Sources": {
      "version": "1.0.0-*",
      "type": "build"
    },
    "NuGet.Commands": {
      "target": "project"
    },
    "System.Runtime.Serialization.Primitives": "4.1.0-rc2-23921"
  },
  "commands": {
    "NuGet.CommandLine.XPlat": "NuGet.CommandLine.XPlat"
  },
  "frameworks": {
    "net46": {
      "frameworkAssemblies": {
        "Microsoft.Build.Framework": "14.0.0",
        "Microsoft.Build": "14.0.0",
        "System.Collections": ""
      }
    },
    "netstandardapp1.5": {
      "imports": [
        "dotnet5.6",
        "dnxcore50",
        "portable-net45+win8"
      ],
      "dependencies": {
        "NETStandard.Library": "1.5.0-rc2-23921"
      }
    }
  }
}